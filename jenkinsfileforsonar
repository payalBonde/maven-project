pipeline{
    agent any
    stages{
        stage ("git cloning"){
            steps{
                git "https://github.com/payalBonde/maven-project"
            }
        }
        stage ("sonar"){
            steps{
                withSonarQubeEnv('sonar') {
                 sh 'mvn sonar:sonar'
                }
            }
        }
    }
}
