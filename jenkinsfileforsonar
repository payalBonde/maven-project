pipeline{
    agent any
    stages{
        stage ("git cloning"){
            steps{
                git "https://github.com/payalBonde/maven-project"
            }
        }
        stage ("sonar"){
            steps{
                withSonarQubeEnv('sonar') {
                 sh 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.6.0.1398:sonar'
                }
            }
        }
    }
}
