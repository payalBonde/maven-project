pipeline{
    agent any
    stages{
        stage ("git cloning"){
            steps{
                git "https://github.com/payalBonde/maven-project"
            }
        }
        stage ("sonar"){
            steps{
                withSonarQubeEnv(credentialsId: 'SonarNew', installationName: 'Sonar New for jenkins') {
                    sh "${scannerHome}/bin/sonar-scanner"
                }
            }
        }
    }
}
